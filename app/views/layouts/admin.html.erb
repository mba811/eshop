<!DOCTYPE html>
<html>
  <head>
    <title>Jdstore13 Admin</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'true' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'true' %>
    <%= csrf_meta_tags %>
  </head>

  <body>
	<div class="container">
		<%= render "common/admin_navbar" %>
		<div class="row">
			<div class="col-md-1">
				<ul class="nav nav-pills nav-stacked" style="max-width: 300px;">
					<li><%= link_to("Products", admin_products_path) %></li>
					<li><%= link_to("Banners", admin_banners_path) %></li>
				</ul>
			</div>

			<div class="col-md-11">
				<%= yield %>
			</div>
		</div>
	</div>
  <%= render "common/admin_footer" %>
  </body>
</html>
